<%= simple_form_for(@complement) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :cpf %>
    <%= f.input :rg %>
    <%= f.association :je ,as: :select%>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
